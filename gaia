#!/usr/bin/env php
<?php

require __DIR__ . '/support/bootstrap.php';


/*
|--------------------------------------------------------------------------
| 获取指令应用实例
|--------------------------------------------------------------------------
| 这里获取指令应用实例
|
*/
$console = \mon\console\App::instance();


/*
|--------------------------------------------------------------------------
| 初始化Gaia
|--------------------------------------------------------------------------
| 这里对Gaia进行初始化
|
*/
\gaia\App::init();


/*
|--------------------------------------------------------------------------
| 执行指令
|--------------------------------------------------------------------------
| 这里执行指令
|
*/
$console->run();
