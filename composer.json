{
    "name": "mongdch/gaia",
    "description": "The Workerman multi worker management framework",
    "type": "project",
    "license": "MIT",
    "homepage": "http://gdmon.com/",
    "keywords": [
        "mon",
        "workerman",
        "high performance",
        "multi process"
    ],
    "authors": [
        {
            "name": "Mon",
            "email": "985558837@qq.com",
            "homepage": "http://www.gdmon.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "workerman/channel": "^1.1",
        "mongdch/gaia-framework": "dev-master",
        "mongdch/mon-http": "dev-master",
        "mongdch/mon-orm": "^2.5",
        "mongdch/mon-auth": "^1.0",
        "mongdch/mon-cache": "dev-master",
        "mongdch/mon-ucenter": "dev-master",
        "mongdch/mon-log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/",
            "support\\": "support/",
            "process\\": "process/"
        },
        "files": [
            "./support/bootstrap.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-package-install": [
            "support\\Plugin::install"
        ],
        "post-package-update": [
            "support\\Plugin::update"
        ],
        "pre-package-uninstall": [
            "support\\Plugin::uninstall"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org/"
        }
    }
}
